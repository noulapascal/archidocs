{% extends 'base.html.twig' %}

{% block title %}Locality{% endblock %}

{% block body %}
    <h1>Locality</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ locality.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ locality.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ locality.description }}</td>
            </tr>
            <tr>
                <th>CreateAt</th>
                <td>{{ locality.createAt ? locality.createAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdateAt</th>
                <td>{{ locality.updateAt ? locality.updateAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('locality_index') }}">back to list</a>

    <a href="{{ path('locality_edit', {'id': locality.id}) }}">edit</a>

    {{ include('locality/_delete_form.html.twig') }}
{% endblock %}
