{% extends 'base.html.twig' %}

{% block title %}Locality{% endblock %}

{% block body %}
    <h1>{% trans %}Locality{% endtrans %}<h1>

    <table class="table">
        <tbody>
            <tr>
                <th>{% trans %}Id{% endtrans %}</th>
                <td>{{ locality.id }}</td>
            </tr>
            <tr>
                <th>{% trans %}Name{% endtrans %}</th>
                <td>{{ locality.name }}</td>
            </tr>
            <tr>
                <th>{% trans %}Description{% endtrans %}</th>
                <td>{{ locality.description }}</td>
            </tr>
            <tr>
                <th>{% trans %}CreateAt{% endtrans %}</th>
                <td>{{ locality.createAt ? locality.createAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>{% trans %}UpdateAt{% endtrans %}</th>
                <td>{{ locality.updateAt ? locality.updateAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('locality_index') }}">back to list</a>

    <a href="{{ path('locality_edit', {'id': locality.id}) }}">{% trans %}edit{% endtrans %}</a>

    {{ include('locality/_delete_form.html.twig') }}
{% endblock %}
